{
  "name": "pushert",
  "version": "1.1.0",
  "description": "Pushert is a Command Line Interface (CLI) application for testing Pusher methods. With Pushert, you can easily configure Pusher credentials, publish messages to Pusher channels, and subscribe to events on Pusher channels.",
  "exports": "./src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "help": "./src/index.js --help",
    "config": "./src/index.js config ",
    "subscribe": "./src/index.js subscribe test test ",
    "broadcast": "./src/index.js publish test test 'test' ",
    "postinstall": "node ./src/index.js config"
  },
  "type": "module",
  "bin": "./src/index.js",
  "keywords": [
    "Pusher CLI",
    "Pusher command-line tool",
    "Pusher methods testing",
    "Real-time messaging with Pusher",
    "Pusher API Key",
    "Pusher API Secret",
    "Publish messages with Pusher",
    "Subscribe to Pusher events",
    "Pusher configuration",
    "Pusher credentials setup",
    "Pusher CLI documentation",
    "Real-time communication with Pusher",
    "Pusher channel management",
    "Pusher event handling",
    "Pusher JavaScript library",
    "Pusher integration",
    "Pusher messaging platform",
    "Pusher CLI installation",
    "Pusher CLI usage",
    "Pusher CLI commands",
    "Pusher CLI tutorial",
    "Pusher CLI setup",
    "Pusher CLI examples",
    "Pusher CLI GitHub repository",
    "Pusher CLI open-source project",
    "Pusher CLI development",
    "Pusher CLI contributions",
    "Pusher CLI license",
    "Pusher CLI acknowledgments",
    "Pusher CLI contact",
    "Pushert",
    "Posher",
    "Pisher"
  ],
  "author": "Feisal Ramadhan Ali",
  "license": "ISC",
  "dependencies": {
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "pusher": "^5.1.3",
    "pusher-client": "^1.1.0"
  }
}
